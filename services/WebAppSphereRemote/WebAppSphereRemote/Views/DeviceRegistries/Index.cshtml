@model IEnumerable<WebAppSphereRemote.Models.DeviceRegistry>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeviceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DesiredProperties)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReportedProperties)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ETags)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DeviceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DesiredProperties)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReportedProperties)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ETags)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id })
        </td>
    </tr>
}

</table>
